{
  "name": "sgo",
  "version": "2.4.3",
  "description": "Setting a directory to a static server.",
  "bin": {
    "sgo": "lib/sgo.js"
  },
  "main": "lib/server.js",
  "typings": "lib/server.d.ts",
  "scripts": {
    "prepare": "npm run build",
    "watch": "npm run build:types -- --watch & npm run watch:ts",
    "build": "npm run build:ts & npm run build:types",
    "build:types": "tsbb types --target ESNEXT",
    "build:ts": "tsbb build",
    "watch:ts": "tsbb watch",
    "test": "tsbb test",
    "coverage": "tsbb test --coverage"
  },
  "keywords": [
    "ssr",
    "sgo",
    "server-go",
    "server",
    "iserver",
    "command",
    "cli",
    "parsing",
    "parser",
    "option",
    "args",
    "argument",
    "tool"
  ],
  "files": [
    "lib",
    "src",
    ".bin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/sgo.git"
  },
  "author": "kenny wang <wowohoo@qq.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.12.5",
    "chokidar": "^3.4.3",
    "ejs": "^3.1.5",
    "fs-extra": "^10.0.0",
    "get-port": "^5.1.1",
    "minimist": "^1.2.5"
  },
  "devDependencies": {
    "@types/ejs": "3.0.5",
    "@types/fs-extra": "9.0.5",
    "@types/minimist": "1.2.1",
    "@types/node": "14.14.14",
    "tsbb": "1.7.9"
  }
}
