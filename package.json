{
  "name": "sgo",
  "version": "2.4.0",
  "description": "Setting a directory to a static server.",
  "bin": {
    "sgo": "lib/sgo.js"
  },
  "main": "lib/server.js",
  "scripts": {
    "prepare": "npm run build",
    "watch": "npm run build:types -- --watch & npm run watch:ts",
    "build": "npm run build:ts & npm run build:types",
    "build:types": "tsbb types --target ESNEXT",
    "build:ts": "tsbb build",
    "watch:ts": "tsbb watch",
    "test": "tsbb test",
    "coverage": "tsbb test --coverage"
  },
  "keywords": [
    "ssr",
    "sgo",
    "server-go",
    "server",
    "iserver",
    "command",
    "cli",
    "parsing",
    "parser",
    "option",
    "args",
    "argument",
    "tool"
  ],
  "files": [
    "lib",
    "src",
    ".bin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/sgo.git"
  },
  "author": "kenny wang <wowohoo@qq.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.11.2",
    "chokidar": "^3.4.2",
    "ejs": "^3.1.5",
    "fs-extra": "^9.0.1",
    "get-port": "^5.1.1",
    "yargs": "^15.4.1"
  },
  "devDependencies": {
    "@types/ejs": "3.0.5",
    "@types/fs-extra": "9.0.1",
    "@types/node": "14.6.0",
    "@types/yargs": "15.0.5",
    "tsbb": "1.7.6"
  }
}
