{
  "name": "sgo",
  "version": "2.2.0",
  "description": "Setting a directory to a static server.",
  "bin": {
    "sgo": "lib/sgo.js"
  },
  "main": "lib/server.js",
  "scripts": {
    "prepare": "npm run build",
    "watch": "npm run build:types -- --watch & npm run watch:ts",
    "build": "npm run build:ts & npm run build:types",
    "build:types": "tsbb types --outDir typings --target ESNEXT",
    "build:ts": "tsbb build",
    "watch:ts": "tsbb watch",
    "test": "tsbb test",
    "coverage": "tsbb test --coverage"
  },
  "keywords": [
    "ssr",
    "sgo",
    "server-go",
    "server",
    "iserver",
    "command",
    "cli",
    "parsing",
    "parser",
    "option",
    "args",
    "argument",
    "tool"
  ],
  "files": [
    "lib",
    "src",
    "typings",
    ".bin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/ssr.git"
  },
  "author": "kenny wang <wowohoo@qq.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "chokidar": "^3.3.1",
    "ejs": "^3.0.1",
    "fs-extra": "^8.1.0",
    "get-port": "^5.1.0",
    "yargs": "^15.0.2"
  },
  "devDependencies": {
    "@types/ejs": "^3.0.0",
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^13.1.1",
    "@types/yargs": "^13.0.4",
    "copyfiles": "^2.1.1",
    "tsbb": "^1.3.9"
  }
}
