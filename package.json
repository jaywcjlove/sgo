{
  "name": "ssr",
  "version": "2.0.0",
  "description": "Setting a directory to a static server.",
  "bin": {
    "ssr": ".bin/ssr"
  },
  "main": "index.js",
  "scripts": {
    "prepare": "npm run build",
    "type-check": "tsc --noEmit",
    "build": "BABEL_ENV=cjs babel src --out-dir lib --extensions \".ts\" --source-maps inline --copy-files --delete-dir-on-start --verbose",
    "tsc": "tsc --removeComments",
    "start": "npm run build -- --watch & npm run type-check -- --watch"
  },
  "keywords": [
    "ssr",
    "server",
    "iserver",
    "command",
    "cli",
    "parsing",
    "parser",
    "option",
    "args",
    "argument",
    "tool"
  ],
  "files": [
    "lib",
    ".bin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/ssr.git"
  },
  "author": "kenny wang <wowohoo@qq.com>",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.4.3",
    "ejs": "^2.6.1",
    "fs-extra": "^7.0.1",
    "get-port": "^5.0.0",
    "yargs": "^13.2.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.4.3",
    "@babel/core": "^7.4.3",
    "@babel/plugin-transform-runtime": "^7.4.3",
    "@babel/preset-env": "^7.4.3",
    "@babel/preset-typescript": "^7.3.3",
    "@types/ejs": "^2.6.3",
    "@types/fs-extra": "^5.0.5",
    "@types/node": "^11.13.7",
    "@types/yargs": "^13.0.0",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5"
  }
}
