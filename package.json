{
  "name": "sgo",
  "version": "2.4.3",
  "description": "Setting a directory to a static server.",
  "bin": {
    "sgo": "lib/sgo.js"
  },
  "main": "lib/server.js",
  "scripts": {
    "prepare": "npm run build",
    "build": "tsbb build --no-esm --disable-babel --entry src/sgo.ts",
    "watch": "tsbb watch --no-esm --disable-babel --entry src/sgo.ts",
    "test": "tsbb test",
    "coverage": "tsbb test --coverage"
  },
  "keywords": [
    "ssr",
    "sgo",
    "server-go",
    "server",
    "iserver",
    "command",
    "cli",
    "parsing",
    "parser",
    "option",
    "args",
    "argument",
    "tool"
  ],
  "files": [
    "lib",
    "src",
    ".bin"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/jaywcjlove/sgo.git"
  },
  "author": "kenny wang <wowohoo@qq.com>",
  "license": "MIT",
  "dependencies": {
    "chokidar": "3.5.2",
    "ejs": "3.1.6",
    "fs-extra": "10.0.0",
    "get-port": "5.1.1",
    "minimist": "1.2.5"
  },
  "devDependencies": {
    "@types/ejs": "3.1.0",
    "@types/fs-extra": "9.0.12",
    "@types/minimist": "1.2.2",
    "@types/node": "16.9.1",
    "tsbb": "3.1.6"
  }
}
